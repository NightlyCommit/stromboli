{
  "name": "stromboli",
  "version": "0.15.0",
  "description": "Simple and efficient component builder",
  "main": "lib/index.js",
  "scripts": {
    "clean": "node tasks/clean.js",
    "prelib": "npm run clean",
    "prepublish": "npm run lib -- --sourceMap false",
    "pretest": "npm run lib",
    "start": "node index.js",
    "lib": "tsc --project .",
    "test": "npm run fastest",
    "fastest": "tape 'test/**/*.js' | tap-mocha-reporter spec",
    "cover": "nyc npm t",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Eric MORAND",
  "dependencies": {},
  "repository": "ericmorand/stromboli",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^10.5.8",
    "coveralls": "^3.0.2",
    "fs-extra": "^7.0.0",
    "nyc": "^12.0.2",
    "tap": "^11.1.5",
    "tap-mocha-reporter": "^3.0.7",
    "tape": "^4.9.1",
    "typescript": "^3.0.1"
  }
}
